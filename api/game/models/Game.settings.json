      "required": false
    },
    "availablePlace": {
      "default": false,
      "type": "boolean"
    },
    "level": {
      "default": "",
      "type": "enumeration",
      "enum": [
        "rookie",
        "pro",
        "expert",
        "all star",
        "hall of fame"
      ]
    },
    "weather": {
      "model": "file",
      "via": "related",
      "plugin": "upload",
      "required": false
    },
    "equipement": {
      "default": "",
      "type": "string"
    },
    "court": {
      "model": "court",
      "via": "games"
    },
    "JoueursEquipe": {
      "default": "",
      "type": "enumeration",
      "enum": [
        "1",
        "2",
        "3",
        "4",
        "5"
